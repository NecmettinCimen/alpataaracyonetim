        <div class="container pd-x-0">
          <div class="d-sm-flex align-items-center justify-content-between mg-b-20 mg-lg-b-25 mg-xl-b-30">
            <div>
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb breadcrumb-style1 mg-b-10">
                  <li class="breadcrumb-item"><a href="/">Alpata</a></li>
                  <li class="breadcrumb-item active pageHeader" aria-current="page"></li>
                </ol>
              </nav>
              <h4 class="mg-b-0 tx-spacing--1 pageHeader"></h4>
            </div>
            <div class="d-none d-md-block">
              <button class="btn btn-sm pd-x-15 btn-primary btn-uppercase mg-l-5" data-toggle="modal" data-target="#genericmodal" onclick="clearForm()">
                <i data-feather="file" class="wd-10 mg-r-5"></i> Ekle
              </button>
            </div>
          </div>

        @RenderBody()
        </div><!-- container -->
      </div>
